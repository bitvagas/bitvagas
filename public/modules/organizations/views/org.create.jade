h4 Create an organization

ul
  li(ng-repeat='error in errors')
    label {{ error }}


form
  ul.create-form
    li
      .create-form__input
      ng-input(theme='fumi' icon='icon icon--fumi mdi mdi-bank' label='Organization' type='text' ng-model='org.NAME' name='NAME')
    li
      .create-form__input
      ng-input(theme='fumi' icon='icon icon--fumi mdi mdi-link' label='Url' type='text' ng-model='org.URL' name='URL')
    hr

  a.btn.btn--medium.btn--primary(ui-sref="dashboard.organization.list") Voltar
  button.btn.btn--medium.btn--primary(type="submit" ng-click='create(org)') Cadastrar Organização
